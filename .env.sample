# Minimal sample .env file for cc_backend
# Copy this to `.env` and replace placeholders with secure values.
# Keep the real `.env` out of source control (it is ignored by `.gitignore`).

# Required
JWT_SECRET=your_jwt_secret_here
GEMINI_API_KEY=your_gemini_api_key_here

# Super admin (created on first server start)
SUPER_ADMIN_USERNAME=admin
SUPER_ADMIN_EMAIL=admin@example.com
SUPER_ADMIN_PASSWORD=change_me_securely

